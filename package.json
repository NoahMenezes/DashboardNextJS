{
  "name": "dashboardtsx",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:backend\" --names \"NEXT,BACKEND\" --prefix-colors \"blue,green\"",
    "dev:next": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:full": "npm run kill-ports && npm run startup-check && concurrently \"npm run dev:next\" \"npm run dev:backend\" --names \"NEXT,BACKEND\" --prefix-colors \"blue,green\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "clean": "rimraf .next node_modules/.cache",
    "clean:all": "rimraf .next node_modules/.cache tsconfig.tsbuildinfo",
    "monitor": "tsx scripts/monitor-memory.ts",
    "kill-ports": "powershell -ExecutionPolicy Bypass -File ./scripts/kill-ports.ps1",
    "startup-check": "node scripts/startup-check.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-oauth/google": "^0.13.4",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/three": "^0.182.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "gl-matrix": "^3.4.4",
    "gsap": "^3.14.2",
    "lucide-react": "^0.562.0",
    "motion": "^12.24.7",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.4.49",
    "rimraf": "^6.1.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
